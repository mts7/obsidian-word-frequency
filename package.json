    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^26.0.0",
    "obsidian": "^1.8.7",
    "prettier": "^3.5.3",
    "rollup": "^4.34.9",
    "rollup-plugin-typescript2": "^0.36.0",
